// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.wordWrap": "off",
  "editor.minimap.enabled": true,
  "editor.formatOnPaste": false,
  "files.exclude": {
      "**/*.pyc": true,
      "**/*.a": true,
      "**/*.class": true,
      "**/*.db": true,
      "**/*.dll": true,
      "**/*.dylib": true,
      "**/*.egg-info": true,
      "**/*.exe": true,
      "**/*.idb": true,
      "**/*.lib": true,
      "**/*.ncb": true,
      "**/*.o": true,
      "**/*.obj": true,
      "**/*.pdb": true,
      "**/*.psd": true,
      "**/*.pyo": true,
      "**/*.sdf": true,
      "**/*.so": true,
      "**/*.sublime-workspace": true,
      "**/*.suo": true,
      "**/.bundle": true,
      "**/.coverage": true,
      "**/.DS_Store": true,
      "**/.expo": true,
      "**/.git": true,
      "**/.githooks": true,
      "**/.hg": true,
      "**/.pygments-cache": true,
      "**/.sass-cache": true,
      "**/.svn": true,
      "**/.tox": true,
      "**/.vagrant": true,
      "**/.venv": true,
      "**/.virtualenv": true,
      "**/__pycache__": true,
      "**/_build": true,
      "**/_deploy": true,
      "**/_site": true,
      "**/backstop_data": true,
      "**/build": true,
      "**/coverage": true,
      "**/css_regression_data": true,
      "**/CVS": true,
      "**/db.sqlite3": true,
      "**/dist": true,
      "**/env": true,
      "**/es5": true,
      "**/node_modules*": true,
      "**/npm-debug.log*": true,
      "**/npm-shrinkwrap.json": true,
      "**/static/**/*.css": false,
      "**/static/**/*.js": false,
      "**/static/wagtailadmin": true,
      "**/tmp": true,
      "**/var": true,
      "**/venv": true,
      "**/www": true,
      "**/yarn.lock": true
  },
  "window.reopenFolders": "all",
  "window.zoomLevel": 0,
  "terminal.external.osxExec": "iTerm.app",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.openPositioning": "right",
  "workbench.sideBar.location": "left",
  "html.format.wrapLineLength": 0,
  "html.format.extraLiners": "object",
  "html.suggest.angular1": false,
  "html.suggest.ionic": false,
  "editor.tabSize": 2,
  "git.enabled": true,
  "git.autorefresh": true,
  "git.autofetch": true,
  "git.countBadge": "all",
  "css.validate": true,
  "scss.validate": true,
  "workbench.iconTheme": "seti",
  "jsAutolint.defaultLinters": ["eslint", "standard"],
  "jsAutolint.enable": true,
  "typescript.check.npmIsInstalled": false
}