
" vim:ft=vim
"------------------------

nnoremap ,ht :-1read $HOME/.vim/snippets/template.html<CR>3jwf>a
nnoremap ,rd :-1read $HOME/.vim/snippets/def.rb<CR>ea<space>
nnoremap ,rc :-1read $HOME/.vim/snippets/class.rb<CR>ea<space>
nnoremap ,re :-1read $HOME/.vim/snippets/each.rb<CR>f\|a
nnoremap ,rm :-1read $HOME/.vim/snippets/map.rb<CR>f\|a

" vim:ft=vim
